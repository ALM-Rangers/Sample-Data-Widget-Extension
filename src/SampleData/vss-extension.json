{
  "manifestVersion": 1,
  "scopes": [ "vso.profile", "vso.work", "vso.work_write", "vso.project_manage" ],
  "description": "Create sample data in your Team Services project.",
  "version": "0.0.1",

  "id": "DEV-SampleDataWidget",
  "name": "DEV://VSTS Sample Data",

  "publisher": "mskold",
  "public": false,

  "targets": [
        {
            "id": "Microsoft.VisualStudio.Services.Cloud"
        }
    ],

  "tags": [
    "Sample",
    "Start"
  ],
  "categories": [
    "Plan and track"
  ],
  "branding": {
    "color": "rgb(51,65,133)",
    "theme": "dark"
  },

  "content": {
    "details": {
      "path": "overview.md"
    },
    "license": {
      "path": "mp_terms.md"
    }
  },
   
  "files": [
    {
      "path": "index.html",
      "addressable": true
    },
    {
      "path": "configuration.html",
      "addressable": true
    },
    {
      "path": "css",
      "addressable": true
    },
    {
      "path": "DefaultTemplates",
      "addressable": true
    },
    {
      "path": "img",
      "addressable": true
    },
    {
      "path": "scripts",
      "addressable": true
    }

  ],
  "icons": {
    "default": "img/logo.png"
  }, 

   "contributions": [
    {
      "id": "sample-data-widget",
      "type": "ms.vss-dashboards-web.widget",
      "targets": [
        "ms.vss-dashboards-web.widget-catalog",
        ".sample-data-widget.configuration"
      ],
      "properties": {
        "name": "Sample Data",
        "description": "Create sample data in your Team Services project.",

        "catalogIconUrl:": "img/logo.png",
        "previewImageUrl": "img/logo.png",

        "uri": "index.html",
        "supportedSizes": [
          {
            "rowSpan": 1,
            "columnSpan": 2
          }
        ],
        "supportedScopes": [ "project_team" ]
      }
    },
    {
      "id": "sample-data-widget.configuration",
      "type": "ms.vss-dashboards-web.widget-configuration",
      "targets": [ "ms.vss-dashboards-web.widget-configuration" ],

      "properties": {
        "name": "Sample Data Configuration",
        "description": "Configures Sample Data",
        "uri": "configuration.html"
      }
    }
  ]


  
}
